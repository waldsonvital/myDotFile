{
    "languageserver": {
        "dart": {
            "command": "dart",
            "args": [
                "/usr/local/opt/dart/libexec/bin/snapshots/analysis_server.dart.snapshot",
                "--lsp"
            ],
            "filetypes": ["dart"],
            "disableDynamicRegister": true,
            "trace.server": "verbose"
        }
    },
    "coc.preferences.rootPatterns":[
        ".vim",
        ".git",
        ".hg",
        ".projections.json",
        "package.json",
        ".vscode",
        ".packages"
    ]
}
